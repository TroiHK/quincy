{{get_svg_icon('logo-white', '0 0 216.33 131.97')}}
<address>
    {{global.street}}, {{global.city}}, {{global.state}} {{global.zip}}<br/>
    {% if global.phone %}
        <a href="tel:{{global.phone|replace({' ': ''})}}" title="phone">
            {{global.phone|replace({' ': '.'})}}
        </a>
    {% endif %}
</address>
<ul class="block-social">
    {% if global.facebook %}
    <li>
        <a href="{{global.facebook}}" title="facebook"><i class="fa fa-facebook-f"></i></a>
    </li>
    {% endif %}
    {% if global.twitter %}
    <li>
        <a href="{{global.twitter}}" title="twitter"><i class="fa fa-twitter"></i></a>
    </li>
    {% endif %}
</ul>